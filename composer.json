{
    "name": "al3x5/xbot",
    "description": "telegram bot",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "al3x5dev",
            "email": "al3x5.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "mk4u/http": "^1.2",
        "monolog/monolog": "^3.6",
        "mk4u/cache": "^0.4",
        "symfony/console": "^7.2"
    },
    "require-dev": {
        "al3x5/xbot-devkit": "^0.2",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Al3x5\\xBot\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "bin": [
        "xbot"
    ],
    "archive": {
        "exclude": [
            "/.gitignore",
            "/.gitattributes",
            "/.github",
            "/docs"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
